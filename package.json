{
  "name": "quipucords-ui",
  "version": "2.0.0",
  "description": "Quipucords UI",
  "author": "Red Hat",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/quipucords/quipucords-ui.git"
  },
  "bugs": {
    "url": "https://github.com/quipucords/quipucords-ui/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "export NODE_ENV=production; webpack --config ./config/webpack.prod.js && npm run dr:surge",
    "start": "export NODE_ENV=development; webpack serve --color --progress --config ./config/webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .ts,.tsx,.js ./src/",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint -- --fix",
    "format": "npm run lint:fix",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config ./config/webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.3",
    "@tanstack/react-query-devtools": "^5.24.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.10",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.6.0",
    "imagemin": "^8.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "mini-svg-data-uri": "^1.4.4",
    "moxios": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.4",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@mturley-latest/react-table-batteries": "^1.0.4",
    "@patternfly/patternfly": "^5.2.0",
    "@patternfly/react-core": "^5.2.0",
    "@patternfly/react-icons": "^5.2.0",
    "@patternfly/react-styles": "^5.2.0",
    "@patternfly/react-table": "^5.2.0",
    "@tanstack/react-query": "^5.24.1",
    "axios": "^1.6.7",
    "detect-browser": "^5.3.0",
    "es5-ext": "^0.10.64",
    "i18next": "^23.10.0",
    "i18next-xhr-backend": "^2.0.1",
    "js-file-download": "^0.4.12",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "react": "^18",
    "react-axe": "^3.5.4",
    "react-dom": "^18",
    "react-i18next": "^14.0.5",
    "react-redux": "^9.1.0",
    "react-router-dom": "6.22.1",
    "sirv-cli": "^2.0.2"
  },
  "packageManager": "yarn@4.1.0"
}
